{
  "name": "order-execution-engine",
  "version": "1.0.0",
  "description": "DEX order execution engine with WebSocket status updates and queue management",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "migration:generate": "drizzle-kit generate",
    "migration:run": "tsx src/database/migrate.ts",
    "db:push": "drizzle-kit push"
  },
  "keywords": [
    "solana",
    "dex",
    "order-execution",
    "websocket",
    "queue"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/websocket": "^10.0.1",
    "bullmq": "^5.13.2",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.35.2",
    "fastify": "^4.28.1",
    "ioredis": "^5.4.1",
    "pg": "^8.13.0",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.5",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "drizzle-kit": "^0.26.2",
    "eslint": "^9.12.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
